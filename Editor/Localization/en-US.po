msgid "HandGesturePatternType:LeftOnly"
msgstr "Left Hand Only"

msgid "HandGesturePatternType:LeftOnly:Desc"
msgstr "A pattern using only the left hand. (8 variations)"

msgid "HandGesturePatternType:RightOnly"
msgstr "Right Hand Only"

msgid "HandGesturePatternType:RightOnly:Desc"
msgstr "A pattern using only the right hand. (8 variations)"

msgid "HandGesturePatternType:BasicRight"
msgstr "Right Hand Priority"

msgid "HandGesturePatternType:BasicRight:Desc"
msgstr "A basic pattern prioritizing the right hand. (16 variations)"

msgid "HandGesturePatternType:BasicLeft"
msgstr "Left Hand Priority"

msgid "HandGesturePatternType:BasicLeft:Desc"
msgstr "A basic pattern prioritizing the left hand. (16 variations)"

msgid "HandGesturePatternType:HandSign"
msgstr "All Hand Combinations"

msgid "HandGesturePatternType:HandSign:Desc"
msgstr "A pattern that allows up to 64 gesture combinations using both hands. (64 variations)"

msgid "HandGesturePatternType:Blending"
msgstr "Both Hands Blending"

msgid "HandGesturePatternType:Blending:Desc"
msgstr "A pattern that blends animations for each hand. Useful for assigning eye and mouth controls to each hand, etc. (64 variations)"

msgid "MenuPatternType"
msgstr "Selected Pattern:"

msgid "MenuPatternType:ExclusiveMenu"
msgstr "Exclusive Menu"

msgid "MenuPatternType:ExclusiveMenu:Desc"
msgstr "A sample that generates an exclusive menu."

msgid "MenuPatternType:BlendingMenu"
msgstr "Blending Menu"

msgid "MenuPatternType:BlendingMenu:Desc"
msgstr "A sample that generates a menu that blends with other expressions."

msgid "OtherPatternType"
msgstr "Selected Pattern:"

msgid "OtherPatternType:HeadContact"
msgstr "Head Contact"

msgid "OtherPatternType:HeadContact:Desc"
msgstr "A sample using contact. Useful for specifying expressions when being patted, etc."

msgid "PatternGUI:AC:Selected"
msgstr "Selected Animator Controller"

msgid "PatternGUI:AC:Desc"
msgstr "Analyzes the animator controller and generates expression controls that behave as closely as possible."

