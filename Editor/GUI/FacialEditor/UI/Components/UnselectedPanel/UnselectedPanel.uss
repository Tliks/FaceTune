/* Unselected Panel Styles */
#unselected-shapes-panel {
    padding-left: 5px;
}

#unselected-label {
    -unity-font-style: bold;
}

#unselected-controls-container {
    flex-direction: row;
    align-items: center;
}

#unselected-list-view {
    border-color: rgba(128, 128, 128, 0.8);
    border-width: 1px;
    border-radius: 5px;
    margin-top: 5px;
} 